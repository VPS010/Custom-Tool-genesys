PORT=3000

# Genesys OAuth credentials - client must provide these securely
GENESYS_CLIENT_ID=your_client_id_here
GENESYS_CLIENT_SECRET=your_client_secret_here

# Region (from URL). Example: usw2 -> login.usw2.pure.cloud and api.usw2.pure.cloud
GENESYS_REGION=

# Contact list id provided by client
CONTACT_LIST_ID=

# Optional: adjust token cache buffer (seconds before expiry to treat as expired)
TOKEN_EXPIRY_BUFFER_SECONDS=30